
@model cspv3.Models.CspApiModels.License.SubscribedSku
@{
    ViewBag.Title = "Customer Licenses";
    Layout = "_CustomerLayout";
}


@inject UserManager<ApplicationUser> userManager;



<div class="row">

    <div class="col-sm-12">


        <div class="page-header">


            <h4 class="page-title"> All Customer Licenses </h4>



            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active" aria-current="page">Customers Licenses</li>
                </ol>
            </nav>

        </div>

        <div class="row grid-margin col-lg-12">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <h6 class="card-title text-md-right">

                          

                        </h6>
                        <br />

                        <div class="panel panel-default card-view">
                            <div class="panel-wrapper collapse in">

                                <div class="panel-body">

                                    <div class="table-wrap">
                                        <div class="table-responsive">
                                            <table id="example" class="table table-hover display  pb-30">
                                                <thead class="blue-grey lighten-4">
                                                    <tr>

                                                        <th>
                                                            License Name
                                                        </th>
                                                        <th>
                                                            Total Licenses
                                                        </th>
                                                        <th>
                                                            Available Licenses
                                                        </th>
                                                        <th>
                                                            Used Licenses
                                                        </th>
                                                        <th>
                                                           


                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var item in Model.Items)
                                                    {
                                                        <tr>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.ProductSku.Name)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.ActiveUnits)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.AvailableUnits)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.ConsumedUnits)
                                                            </td>
                                                         
                                                          


                                                        </tr>

                                                    }
                                                </tbody>
                                            </table>
                                        </div>
                                        <br />

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>