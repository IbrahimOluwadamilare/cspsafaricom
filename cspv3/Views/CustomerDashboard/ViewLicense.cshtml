

@model List<cspv3.Models.CspApiModels.Licenses.Item>
@{
    ViewBag.Title = "View License";
    Layout = "_CustomerLayout";
}
@using X.PagedList.Mvc.Core
@using Microsoft.AspNetCore.Http;




@inject UserManager<ApplicationUser> userManager;


<div class="row">

    <div class="col-sm-12">


        <div class="page-header">


            <h4 class="page-title"> All Licences </h4>



            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#">Dashboard</a></li>
                    <li class="breadcrumb-item active" aria-current="page">All Licences</li>
                </ol>
            </nav>

        </div>

        <div class="row grid-margin col-lg-12">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">

                        <br />

                        <div class="panel panel-default card-view">
                            <div class="panel-wrapper collapse in">

                                <div class="panel-body">

                                    <div class="table-wrap">
                                        <div class="table-responsive">
                                            <table id="datable_1" class="table table-hover display  pb-30">
                                                <thead class="blue-grey lighten-4">
                                                    <tr>
                                                        <th>
                                                            Id
                                                        </th>
                                                        <th>
                                                            Name
                                                        </th>



                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var item in Model)
                                                    {
                                                        <tr>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.ProductSku.Id)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.ProductSku.Name)
                                                            </td>




                                                        </tr>

                                                    }
                                                </tbody>
                                            </table>
                                        </div>
                                        <br />

                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
