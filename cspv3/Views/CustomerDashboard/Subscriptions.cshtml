
@model List<cspv3.Models.AzureApiModels.ASubResponse.Item>
@{
    ViewBag.Title = "Subscription";
    Layout = "_CustomerLayout";
}

<div class="row">

    <div class="col-sm-12">


        <div class="page-header">


            <h4 class="page-title"> My Subscriptions </h4>



            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#">Dashboard</a></li>
                    <li class="breadcrumb-item active" aria-current="page">My Subscriptions</li>
                </ol>
            </nav>

        </div>

        <div class="row grid-margin col-lg-12">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">

                        <br />

                        <div class="panel panel-default card-view">
                            <div class="panel-wrapper collapse in">

                                <div class="panel-body">

                                    <div class="table-wrap">
                                        <div class="table-responsive">
                                            <table id="example" class="table table-hover display  pb-30">
                                                <thead class="blue-grey lighten-4">
                                                    <tr>
                                                        <th>
                                                            Order ID
                                                        </th>
                                                        <th>
                                                            Subscription ID
                                                        </th>
                                                        <th>
                                                            Name
                                                        </th>
                                                        <th>
                                                            Quantity
                                                        </th>
                                                        <th>
                                                            Billing Cycle
                                                        </th>
                                                        <th>
                                                            Creation Date
                                                        </th>
                                                        <th>
                                                            Effective Start Date
                                                        </th>
                                                        <th>
                                                            Auto Renewal enabled
                                                        </th>


                                                        <th>
                                                            Status
                                                        </th>
                                                        <th>
                                                            Subscription End Date
                                                        </th>
                                                        <th>
                                                            Action
                                                        </th>

                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var item in Model)
                                                    {
                                                        <tr>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.OrderId)
                                                            </td>

                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Id)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.OfferName)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Quantity)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.BillingCycle)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.CreationDate)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.EffectiveStartDate)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.AutoRenewEnabled)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Status)
                                                            </td>

                                                            <td>
                                                                @item.EffectiveStartDate.AddDays(30)
                                                            </td>



                                                            <td>
                                                               
                                                            </td>
                                                        </tr>

                                                    }
                                                </tbody>
                                            </table>
                                        </div>
                                        <br />
                                   
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </div>
    </div>
